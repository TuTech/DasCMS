<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
    <head>
    	<base href="{SEnviornment:BaseURL}" />
{ClientData}
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <title>Bambus CMS: {ApplicationTitle} - {configuration:sitename}</title>
    </head>
    <body onload="body_load();" onunload="BCMSDestroy();">
    	<div id="BambusHeader">
	    	<div id="BambusApplicationPath">
	    	
	    		<div class="BCMSPath BCMSPathRoot">
	    			<table class="BCMSPathCurrent"><tr><td><img src="{cms:BambusLogo}" alt="Bambus CMS" title="{cms:version}" /></td></tr></table>
	    		</div>
	    		<div class="BCMSPath BCMSPathObject" onclick="">
	     			<table class="BCMSPathCurrent"><tr><td><img src="{configuration:logo}" alt="" title="" /></td><td>{configuration:sitename}</td></tr></table>    		
	     		</div>
	   			<div class="BCMSPath BCMSPathObject" id="BCMSPathAppSect" onclick="BCMSApplicationSwitcher(this);OBJ_ofd.close();">
	    			<table class="BCMSPathCurrent"><tr><td><img src="{cms:ApplicationLogo}" alt="" title="{translate:show_all_applications}" /></td><td>{cms:ApplicationTitle}</td></tr></table>
	    		</div>
	   			<div class="BCMSPath BCMSPathObject" id="BCMSPathTabSect" onclick="BCMSTabSwitcher(this);OBJ_ofd.close();">
	     			<table class="BCMSPathCurrent"><tr><td><img src="{cms:ApplicationTabLogo}" alt="" title="{translate:show_all_tabs}" /></td><td>{cms:ApplicationTabTitle}</td></tr></table>
	    		</div>
	  			<div class="BCMSPath BCMSPathObject" id="BCMSPathObjectSect" onclick="BCMSObjSwitcher(this, OBJ_ofd);OBJ_ofd.toggle();">
	     			<table class="BCMSPathCurrent"><tr><td><img src="{cms:CurrentObjectIcon}" alt="" title="{translate:show_all_files}" /></td><td>{cms:CurrentObjectTitle}</td></tr></table>
	    		</div>
	    	</div>
			<img id="Logout" src="{logoutImage}" alt="{translate:logout}" title="{translate:logout}" onclick="BCMSLogout('{translate:exit_management_area}', '{configuration:confirm_for_exit}', '{configuration:logout_on_exit}')" />
			<br class="clear" />
			{TaskBar}
		</div>
 		{NotificationCenter:notifier}
        <br class="clear" />
		<div style="display:none;position:fixed;z-index:5000;right:85px;top:0px;border:1px solid black;background:#2e3436;color:white;padding:5px;">
			run: {cms:runtime}sec<br />
			mem: {cms:memusage}
		</div>
        <div id="bambusJAX"></div>
    </body>
</html>